{
    "name": "magento/marketplace-store",
    "description": "Magento Marketplace Store",
    "type": "project",
    "version": "2.4.2",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "fastly-magento2": {
            "type": "vcs",
            "url": "https://github.com/fastly/fastly-magento2"
        },
        "algoliasearch-magento-2": {
            "type": "vcs",
            "url": "https://github.com/magento-marketplace-store/algoliasearch-magento-2"
        },
        "repo": {
            "type": "composer",
            "url": "https://repo.magento.com",
            "exclude": ["bazaarvoice/bazaarvoice-magento2-ext"]
        }
    },
    "require": {
        "algolia/algoliasearch-magento-2": "1.13.1.7",
        "bazaarvoice/bazaarvoice-magento2-ext": "^7.2.5",
        "fastly/magento2": "^1.2.131",
        "magento/ece-tools": "^2002.1.0",
        "magento/product-enterprise-edition": "2.4.2-p1",
        "magento/product-recommendations": "^3.0",
        "makotokw/twient": "^0.7.0",
        "paypal/module-braintree": "^4.1",
        "sixtomartin/onelogin-module-saml2-extend": "^1.6"
    },
    "replace": {
        "magento/module-adobe-stock-asset": "*",
        "magento/module-adobe-stock-asset-api": "*",
        "magento/module-adobe-stock-image": "*",
        "magento/module-adobe-stock-image-admin-ui": "*",
        "magento/module-adobe-stock-image-api": "*",
        "magento/module-adobe-stock-client": "*",
        "magento/module-adobe-stock-client-api": "*",
        "magento/module-adobe-stock-admin-ui": "*",
        "magento/module-adobe-ims": "*",
        "magento/module-adobe-ims-api": "*",
        "magento/module-login-as-customer": "*",
        "magento/module-login-as-customer-admin-ui": "*",
        "magento/module-login-as-customer-api": "*",
        "magento/module-login-as-customer-frontend-ui": "*",
        "magento/module-login-as-customer-graph-ql": "*",
        "magento/module-login-as-customer-log": "*",
        "magento/module-login-as-customer-logging": "*",
        "magento/module-login-as-customer-page-cache": "*",
        "magento/module-login-as-customer-quote": "*",
        "magento/module-login-as-customer-sales": "*",
        "magento/module-login-as-customer-website-restriction": "*",
        "magento/module-login-as-customer-assistance": "*",
        "magento/google-shopping-ads": "*",
        "magento/module-cardinal-commerce": "*",
        "magento/module-admin-analytics": "*",
        "magento/module-fedex": "*",
        "magento/module-usps": "*",
        "magento/module-ups": "*",
        "magento/module-dhl": "*",
        "magento/module-offline-payments": "*",
        "magento/module-offline-shipping": "*",
        "magento/module-weee": "*",
        "magento/module-weee-graph-ql": "*",
        "magento/module-weee-staging": "*",
        "magento/module-swagger": "*",
        "magento/module-swagger-webapi": "*",
        "magento/module-swagger-webapi-async": "*",
        "magento/module-scalable-inventory": "*",
        "magento/module-scalable-oms": "*",
        "magento/module-scalable-checkout": "*",
        "magento/module-re-captcha-version-3-invisible": "*",
        "magento/module-re-captcha-version-2-invisible": "*",
        "magento/module-re-captcha-version-2-checkbox": "*",
        "amzn/login-with-amazon-module": "*",
        "amzn/amazon-pay-module": "*",
        "amzn/amazon-pay-and-login-with-amazon-core-module": "*",
        "klarna/module-core": "*",
        "klarna/module-kp": "*",
        "klarna/module-onsitemessaging": "*",
        "klarna/module-ordermanagement": "*",
        "klarna/module-kp-graph-ql": "*",
        "vertex/module-address-validation": "*",
        "vertex/module-tax": "*",
        "vertex/module-tax-staging": "*",
        "yotpo/magento2-module-yotpo-reviews": "*"
    },
    "suggest": {
        "ext-pcntl": "Need for run processes in parallel mode"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/",
            "Magento\\AppStore\\MagentoCloud\\": "dev/tools/AppStore/MagentoCloud/src/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php",
            "dev/tools/AppStore/MagentoCloud/registration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "component_paths": {
            "trentrichardson/jquery-timepicker-addon": "lib/web/jquery/jquery-ui-timepicker-addon.js",
            "components/jquery": [
                "lib/web/jquery.js",
                "lib/web/jquery/jquery.min.js",
                "lib/web/jquery/jquery-migrate.js"
            ],
            "blueimp/jquery-file-upload": "lib/web/jquery/fileUploader",
            "components/jqueryui": [
                "lib/web/jquery/jquery-ui.js"
            ],
            "twbs/bootstrap": [
                "lib/web/jquery/jquery.tabs.js"
            ],
            "tinymce/tinymce": "lib/web/tiny_mce_4"
        },
        "magento-force": true,
        "magento-deploystrategy": "copy"
    }
}
